{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "java",
        "name": "Launch Spring Boot",
        "request": "launch",
        "mainClass": "com.todoapp.TodoApplication",
        "projectName": "todo-app"
      },
      {
        "type": "java",
        "name": "Launch JavaFX UI",
        "request": "launch",
        "mainClass": "com.todoapp.ui.TodoAppUI",
        "projectName": "todo-app",
        "vmArgs": "--module-path /path/to/javafx-sdk-19/lib --add-modules javafx.controls,javafx.fxml"
      }
    ]
  }
  